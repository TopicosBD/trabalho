<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="sistemaclinica"/>
  <table-filter match-name="medico"/>
  <table-filter match-name="cargo"/>
  <table-filter match-name="atestado"/>
  <table-filter match-name="riscos_ocupacionais"/>
  <table-filter match-name="empresa"/>
  <table-filter match-name="tipo_exame"/>
  <table-filter match-name="cargo_has_riscos_ocupacionais"/>
  <table-filter match-name="empresa_has_exame"/>
  <table-filter match-name="funcionario"/>
  <table-filter match-name="exame_has_atestado"/>
  <table-filter match-name="funcionario_has_exame"/>
  <table-filter match-name="exame"/>
  <table-filter match-name="resultado_exame"/>
</hibernate-reverse-engineering>
